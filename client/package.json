{
  "name": "client",
  "author": "Mathieu MARCHOIS",
  "version": "1.0.0",
  "scripts": {
    "dev": "sapper dev",
    "watch:tailwind": "postcss static/css/tailwind.css -o static/css/index.css -w",
    "build:tailwind": "NODE_ENV=production postcss static/css/tailwind.css -o static/css/index.css",
    "build": "npm run build:tailwind && sapper build --legacy",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "test": "run-p --race dev cy:run",
    "format": "prettier -w src/**/*.{js,svelte}",
    "lint": "eslint ./*.js ./src/**/*.js ./src/components/**/*.svelte",
    "test-unit": "jest",
    "coverage": "jest --coverage"
  },
  "dependencies": {
    "@fullcalendar/core": "^4.4.2",
    "@fullcalendar/daygrid": "^4.4.2",
    "@fullcalendar/interaction": "^4.4.2",
    "@fullhuman/postcss-purgecss": "^3.0.0",
    "axios": "^0.21.4",
    "compression": "^1.7.1",
    "cookie-parser": "^1.4.5",
    "cssnano": "^4.1.10",
    "date-fns": "^2.14.0",
    "express": "^4.17.1",
    "filesize": "^6.1.0",
    "iso-country-codes": "0.0.5",
    "js-cookie": "^2.2.1",
    "serialize-javascript": ">=3.1.0",
    "shortid": "^2.2.15",
    "sirv": "^0.4.6",
    "svelte-i18n": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/runtime": "^7.10.4",
    "@beyonk/sapper-rbac": "0.0.3",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^6.1.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@tailwindcss/custom-forms": "^0.2.1",
    "@testing-library/jest-dom": "^5.11.0",
    "@testing-library/svelte": "^3.0.0",
    "babel-jest": "^25.5.1",
    "color": "^3.1.3",
    "eslint": "^7.11.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-svelte3": "^2.7.3",
    "install": "^0.13.0",
    "jest": "^25.5.2",
    "npm": "^6.14.8",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^8.1.0",
    "prettier": "^2.1.2",
    "prettier-plugin-svelte": "^1.4.0",
    "rollup": "^1.32.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-svelte": "^5.2.3",
    "rollup-plugin-terser": "^7.0.2",
    "sapper": "^0.27.16",
    "svelte": "^3.24.0",
    "svelte-jester": "^1.0.6",
    "tailwindcss": "^1.9.2",
    "tailwindcss-multi-theme": "^1.0.3"
  }
